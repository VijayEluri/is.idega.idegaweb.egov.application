<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app>

    <!-- The ApplicationUrlRedirector Filter -->
    <filter>
        <filter-name>ApplicationUrlRedirector</filter-name>
        <filter-class>is.idega.idegaweb.egov.application.servlet.filter.ApplicationUrlRedirector</filter-class>
    </filter>

    <filter-mapping>
        <filter-name>ApplicationUrlRedirector</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>

</web-app>